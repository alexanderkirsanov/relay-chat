{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "quotes": [2, "single", "avoid-escape"],
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "no-else-return": 1,
    "no-var":"error",
    "no-extra-semi": 2,
    "no-param-reassign": [2,{"props":false}]
  },
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jasmine": true,
    "jest": true
  }
}